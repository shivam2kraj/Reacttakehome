{
    "include": [
        "./src/**/*"
    ],
    "compilerOptions": {
        "allowJs": true,
    "jsx": "react-jsx",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "strict": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "baseUrl": "./",
        "paths": {
          "*": ["node_modules/*", "src/*"]
        },
        
    }
}